    "from pathlib import Path\n",
    "\n",
    "raw_dir = Path('data/raw')\n",
    "for item in sorted(raw_dir.glob('*')):\n",
    "    if item.is_file():\n",
    "        print(f'File: {item.name}')\n",
    "    else:\n",
    "        print(f'Folder: {item.name}/')"
